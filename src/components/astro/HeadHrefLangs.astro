---
import { getLocaleUrl, localeKeys, defaultLocale } from "astro-i18n-aut";
---

<Fragment>
    <link rel="alternate" hreflang="x-default" href={getLocaleUrl(Astro.url, defaultLocale)} />
    {
      localeKeys.map((supportedLocale) => (
          <link rel="alternate" hreflang={supportedLocale} href={getLocaleUrl(Astro.url, supportedLocale)}/>
        ))
    }
</Fragment>
